        "@nanostores/react": "^0.7.1",
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19",
        "astro": "^4.3.7",
        "classnames": "^2.5.1",
        "nanostores": "^0.9.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "prettier-plugin-astro": "^0.13.0"
    }
