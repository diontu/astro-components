{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": ".",
        "paths": {
            "@public/*": ["public/*"],
            "@components-react/*": ["src/components/react/*"],
            "@components/*": ["src/components/*"],
            "@pre-built-sections/*": ["src/sections/pre-built/*"],
            "@sections": ["src/sections/index.ts"],
            "@pre-built-section-wrappers/*": [
                "src/section-wrappers/pre-built/*"
            ],
            "@section-wrappers": ["src/section-wrappers/index.ts"],
            "@layouts/*": ["src/layouts/*"],
            "@constants": ["src/constants.ts"],
            "@src/*": ["src/*"]
        }
    }
}
